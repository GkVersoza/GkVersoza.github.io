    "skills.html": {
      "settings": {
        "title": "Skills",
        "order": 4,
        "meta_descr": "List of skills professionally used in Gk Versoza's businesses and companies work",
        "meta_image": "@PROJECT_PATH@/assets/images/mbr-1160x773.png"
      },
      "components": [
        {
          "alias": false,
          "_styles": {
            ".mbr-section": {
              "text-align": "center",
              "color": "#fff"
            },
            "p": {
              "display": "inline",
              "font-size": "unit(@sizetext, px)",
              "color": "#fff",
              "& when (@showneon)": {
                "& when (@effectstyle = 'stroke')": {
                  "text-shadow": "-1px -1px 0 @textshadow, 1px -1px 0 @textshadow, -1px 1px 0 @textshadow, 1px 1px 0 @textshadow, 1px 1px 10px #141414"
                },
                "& when (@effectstyle = 'neon')": {
                  "text-shadow": "0 0 2px @textshadow, 0 0 30px @textshadow, 0px 0px 5px @textshadow, 0 0 150px @textshadow"
                },
                "& when (@effectstyle = 'offset')": {
                  "text-shadow": "4px 4px 0px @textshadow, 7px 7px 0px rgba(0, 0, 0, 0.2)"
                },
                "& when (@effectstyle = '3d')": {
                  "text-shadow": "@textshadow 3px 5px 0, @dcolor1 4px 6px 3px, @dcolor2 8px 11px 10px"
                }
              }
            },
            "& when (@Style = 'defile')": {
              ".marquee-rtl": {
                "width": "unit(@wdtext, %)",
                "max-width": "100%",
                "overflow": "hidden"
              },
              ".marquee-rtl > :first-child": {
                "display": "inline-block",
                "padding-right": "2em",
                "padding-left": "100%",
                "white-space": "nowrap",
                "animation": "defilement-rtl (@speedtext * 1s) infinite linear",
                "animation-iteration-count": "(@counttext * 1)"
              },
              "& when (@moussehover)": {
                ".marquee-rtl:hover :first-child": {
                  "animation-play-state": "paused"
                }
              },
              "@keyframes defilement-rtl": {
                "0%": {
                  "transform": "translate3d(0, 0, 0)"
                },
                "100%": {
                  "transform": "translate3d(-100%, 0, 0)"
                }
              },
              ".media-container-row .mbr-text": {
                "color": "#ffffff"
              }
            },
            "& when (@Style = 'blink')": {
              ".marquee-rtl": {
                "animation": "Test (@speedtext * 1s) infinite",
                "padding-right": "unit(@rightpad, %)",
                "padding-left": "unit(@leftpad, %)",
                "animation-iteration-count": "(@counttext * 1)"
              },
              "@keyframes Test": {
                "0%": {
                  "opacity": "1"
                },
